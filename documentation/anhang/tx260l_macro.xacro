         </inertial>
      </link>
      <link name="${prefix}link_3">
         <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
               <mesh filename="package://staubli_tx260l_support/meshes/tx260l/visual/link_3.stl" />
            </geometry>
