            </geometry>
            <xacro:material_staubli_ral_grey_aluminium />
         </visual>
         <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
               <mesh filename="package://staubli_tx260l_support/meshes/tx260l/collision/link_5.stl" />
            </geometry>
         </collision>
         <inertial>
            <mass value="0.123625" />
            <origin xyz="0.000190 0.000001 0.015244" rpy="0.0 0.0 0.0" />
            <inertia ixx="0.000000" ixy="0.000000" ixz="0.000000" iyy="0.000000" iyz="0.000000" izz="0.000000" />
