<launch>

<!-- Launch the Driver for ABB -->
  <include file="$(find abb_driver)launch/robot_interface.launch" >
    <arg name="robot_ip" value="10.0.0.206" />
    </include>

  <!-- include file="$(find cell_irb120_moveit_config)/launch/demo.launch"/ -->  

  <include file="$(find cell_irb120_moveit_config)/launch/planning_context.launch" >
    <arg name="load_robot_description" value="true" />
    </include>



  
  <include file="$(find cell_irb120_moveit_config)/launch/move_group.launch" >
    <arg name="allow_trajectory_execution" value="true" />
  </include>

  <include file="$(find cell_irb120_moveit_config)/launch/moveit_rviz.launch" >
    <arg name="config" value="true" />
  </include>

  <node name="penAssembly" pkg="cell_core" type="penAssembly" respawn="false" output="screen"  />

  <!--include file="$(find http_server)/launch/httpNode.launch" /-->



</launch>
